{
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "plugins": ["react", "react-hooks"],
    "env": {
      "browser": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "react/react-in-jsx-scope": "error",
      "no-unused-vars": "warn",
      "react/prop-types": "off"
    }
  }